              ".first-col .mbr-section-btn": {
                "padding-top": "1rem"
              }
            },
            ".mbr-text": {
              "margin": "0"
            },
            ".iconfont-wrapper": {
              "display": "inline-block",
              "width": "calc(~\"@{iconsSize}rem + 1rem\")",
              "height": "calc(~\"@{iconsSize}rem + 1rem\")"
            },
            ".icons-list": {
              "padding-top": "1rem",
              "a:not(:last-child)": {
                "margin-right": ".5rem"
              },
              "span": {
                "border-radius": "50%",
                "padding": "0.5rem 0.6rem",
                "color": "@iconsColor",
                "border": "1px solid @iconsColor",
                "transition": "all .3s",
                "width": "calc(~\"@{iconsSize}rem + 1rem\")",
                "height": "calc(~\"@{iconsSize}rem + 1rem\")",
                "font-size": "~\"@{iconsSize}rem\"",
                "&:hover": {
                  "color": "@iconsHover",
                  "-webkit-transform": "scale(1.2, 1.2)",
                  "transform": "scale(1.2, 1.2)"
                }
              }
            },
            ".field": {
              "padding": "0"
            },
            ".dragArea": {
              "margin": "0",
              "justify-content": "center"
            },
            ".field-input": {
              "background": "transparent",
              "padding": ".6rem",
              "height": "100%",
              "color": "@inputColor"
            },
            ".mbr-form .mbr-section-btn": {
              "margin": "0",
              ".btn": {
                "height": "100%",
                "padding": "1rem"
              }
            },
            "@media (max-width: 991px)": {
              ".md-pb": {
                "padding-bottom": "2rem"
              }
            },
            "@media (max-width: 767px)": {
              ".sm-pb": {
                "padding-bottom": "2rem"
              },
              ".mbr-form .mbr-section-btn": {
                "margin-top": ".5rem"
              }
            }
